IO.print(123.ceil)      // expect: 123
IO.print((-123).ceil)   // expect: -123
IO.print(0.ceil)        // expect: 0
IO.print((-0).ceil)     // expect: -0
IO.print(0.123.ceil)    // expect: 1
IO.print(12.3.ceil)     // expect: 13
IO.print((-0.123).ceil) // expect: -0
IO.print((-12.3).ceil)  // expect: -12
